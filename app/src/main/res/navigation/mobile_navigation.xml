<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.ffccloud.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_home_to_nav_start_day"
            app:destination="@id/nav_start_day"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_nav_home_to_nav_target_main"
            app:destination="@id/nav_target_main"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            />
        <action
            android:id="@+id/action_nav_home_to_fragment_menu"
            app:destination="@id/fragment_menu"

            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_nav_home_to_nav_target_sub_menu"
            app:destination="@id/nav_target_sub_menu"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            />
        <action
            android:id="@+id/action_nav_home_to_doctorListFragment"
            app:destination="@id/doctorListFragment" />
        <action
            android:id="@+id/action_nav_home_to_showRouteFragment"
            app:destination="@id/showRouteFragment" />
        <action
            android:id="@+id/action_nav_home_to_meetingFragment"
            app:destination="@id/meetingFragment" />
        <action
            android:id="@+id/action_nav_home_to_mapsFragment"
            app:destination="@id/mapsFragment" />

        <action
            android:id="@+id/action_nav_home_to_nav_expense_list"
            app:destination="@id/nav_expense_list" />
        <action
            android:id="@+id/action_nav_home_to_tableLayout"
            app:destination="@id/tableLayout" />


    </fragment>

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.ffccloud.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.ffccloud.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />

    <fragment
        android:id="@+id/nav_start_day"
        android:name="com.example.ffccloud.TargetMenu.StartDayFragment"
        android:label="Start Day"
        tools:layout="@layout/fragment_start_day" >
        <action
            android:id="@+id/action_nav_start_day_to_nav_target_main"
            app:destination="@id/nav_target_main"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/nav_home"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_nav_start_day_to_nav_attendance"
            app:destination="@id/nav_attendance"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>

    <fragment
        android:id="@+id/nav_target_main"
        android:name="com.example.ffccloud.TargetMenu.TargetMainFragment"
        android:label="Target Main"
        tools:layout="@layout/fragment_target_main" >
        <action
            android:id="@+id/action_nav_target_main_to_nav_target_sub_menu"
            app:destination="@id/nav_target_sub_menu"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/nav_home"
            app:popUpToInclusive="true"/>


        <action
            android:id="@+id/action_nav_target_main_to_fragment_menu"
            app:destination="@id/fragment_menu"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/nav_home"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_target_main_to_showRouteFragment"
            app:destination="@id/showRouteFragment" />
    </fragment>

    <fragment
        android:id="@+id/nav_target_sub_menu"
        android:name="com.example.ffccloud.Target.TargetPreMenuFragment"
        android:label="Target Sub Menu"
        tools:layout="@layout/fragment_target_sub_menu" >
        <action
            android:id="@+id/action_nav_target_sub_menu_to_nav_target"
            app:destination="@id/target_fragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"

            />
        <action
            android:id="@+id/action_nav_target_sub_menu_to_fragment_menu"
            app:destination="@id/fragment_menu"
            />
    </fragment>

    <fragment
        android:id="@+id/target_fragment"
        android:name="com.example.ffccloud.Target.TargetFragment"
        android:label="Target"
        tools:layout="@layout/fragment_target" >
        <action
            android:id="@+id/action_target_to_targetPostMenuFragment"
            app:destination="@id/targetPostMenuFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_target_fragment_to_addworkPlanFragment"
            app:destination="@id/addworkPlanFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/nav_product_info"
        android:name="com.example.ffccloud.Target.Visit.ProductInfoFragment"
        android:label="Product Info"
        tools:layout="@layout/fragment_product_info" />

    <fragment
        android:id="@+id/nav_history"
        android:name="com.example.ffccloud.Target.Visit.HistoryFragment"
        android:label="History"
        tools:layout="@layout/fragment_history" >

        <argument
            android:name="doc_id"
            app:argType="long"
            android:defaultValue="0L" />
    </fragment>

    <fragment
        android:id="@+id/nav_product_slider"
        android:name="com.example.ffccloud.Target.Visit.ProductSliderFragment"
        android:label="Product Slider"
        tools:layout="@layout/fragment_product_slider" />


    <fragment
        android:id="@+id/fragment_menu"
        android:name="com.example.ffccloud.TargetMenu.TargetMenuFragment"
        android:label="Office"
        tools:layout="@layout/fragment_target_menu" >
        <action
            android:id="@+id/action_fragment_menu_to_nav_target_sub_menu"
            app:destination="@id/nav_target_sub_menu"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/nav_home"
            app:popUpToInclusive="true"/>
        <argument
            android:name="selectedMenu"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="" />
        <action
            android:id="@+id/action_fragment_menu_to_showRouteFragment"
            app:destination="@id/showRouteFragment" />
    </fragment>

    <fragment
        android:id="@+id/Scan"
        android:name="com.example.ffccloud.Target.ScanFragment"
        android:label="Scan Fragment"
        tools:layout="@layout/fragment_scan" />

    <fragment
        android:id="@+id/nav_visit"
        android:name="com.example.ffccloud.Target.Visit.VisitFragment"
        android:label="Visit"
        tools:layout="@layout/fragment_visit" />

    <fragment
        android:id="@+id/nav_sample"
        android:name="com.example.ffccloud.Target.Sample.SampleFragment"
        android:label="Sample"
        tools:layout="@layout/fragment_sample" />

    <fragment
        android:id="@+id/nav_followup"
        android:name="com.example.ffccloud.Target.FollowUp.FollowUpFragment"
        android:label="Follow Up"
        tools:layout="@layout/fragment_follow_up" />

    <fragment
        android:id="@+id/nav_recovery"
        android:name="com.example.ffccloud.Target.Recovery.RecoveryFragment"
        android:label="Recovery"
        tools:layout="@layout/fragment_recovery" />

    <fragment
        android:id="@+id/nav_attendance"
        android:name="com.example.ffccloud.AttendanceActivity.AttendanceFragment"
        android:label="Attendance"
        tools:layout="@layout/fragment_attendance" >
        <action
            android:id="@+id/action_nav_attendance_to_nav_target_main"
            app:destination="@id/nav_target_main"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            />
    </fragment>
    <fragment
        android:id="@+id/targetPostMenuFragment"
        android:name="com.example.ffccloud.Target.TargetFullInfoFragment"
        android:label="Post Menu"
        tools:layout="@layout/fragment_target_full_info" >
        <argument
            android:name="DoctorModel"
            app:argType="com.example.ffccloud.DoctorModel" />
        <action
            android:id="@+id/action_targetPostMenuFragment_to_activityTaskFragment"
            app:destination="@id/activityTaskFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            />
        <action
            android:id="@+id/action_targetPostMenuFragment_to_nav_history"
            app:destination="@id/nav_history"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>


    </fragment>
    <fragment
        android:id="@+id/addworkPlanFragment"
        android:name="com.example.ffccloud.Target.AddworkPlanFragment"
        android:label="Add Work Plan"
        tools:layout="@layout/fragment_addwork_plan" >
        <action
            android:id="@+id/action_addworkPlanFragment_to_target_fragment"
            app:destination="@id/target_fragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/target_fragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_addworkPlanFragment_to_addWorkPlanDialogFragment"
            app:destination="@id/addWorkPlanDialogFragment"

            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <argument
            android:name="areaIds"
            app:argType="string"
            android:defaultValue="none" />
        <argument
            android:name="areaTittles"
            app:argType="string"
            android:defaultValue="none" />
        <argument
            android:name="key"
            app:argType="integer"
            android:defaultValue="1" />


    </fragment>
    <dialog
        android:id="@+id/addWorkPlanDialogFragment"
        android:name="com.example.ffccloud.Target.AddWorkPlanDialogFragment"
        android:label="AddWorkPlanDialogFragment"
        tools:layout="@layout/custom_add_workplan_dialog"
        >
        <argument
            android:name="key"
            app:argType="integer"
            android:defaultValue="1"
            />
        <action
            android:id="@+id/action_addWorkPlanDialogFragment_to_addworkPlanFragment"
            app:destination="@id/addworkPlanFragment" />
        <argument
            android:name="dataIds"
            app:argType="string"
            android:defaultValue="none" />


    </dialog>
    <fragment
        android:id="@+id/doctorListFragment"
        android:name="com.example.ffccloud.Target.DoctorListFragment"
        android:label="Doctor List"
        tools:layout="@layout/fragment_doctor_list" >
        <action
            android:id="@+id/action_doctorListFragment_to_filteredDocFullInfoFragment"
            app:destination="@id/filteredDocFullInfoFragment"

            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            />
        <action
            android:id="@+id/action_doctorListFragment_to_doctorFormFragment"
            app:destination="@id/doctorFormFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            />
    </fragment><action android:id="@+id/action_global_doctorListFragment" app:destination="@id/doctorListFragment"/>
    <fragment
        android:id="@+id/filteredDocFullInfoFragment"
        android:name="com.example.ffccloud.Target.FilteredDocFullInfoFragment"
        android:label="Doctor Info"
        tools:layout="@layout/fragment_filtered_doc_full_info" >
        <argument
            android:name="doctorId"
            app:argType="integer"
            android:defaultValue="0" />
        <action
            android:id="@+id/action_filteredDocFullInfoFragment_to_doctorFormFragment"
            app:destination="@id/doctorFormFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            />
    </fragment>
    <fragment
        android:id="@+id/doctorFormFragment"
        android:name="com.example.ffccloud.Target.DoctorFormFragment"
        android:label="Doctor Form"
        tools:layout="@layout/fragment_doctor_form" >
        <action
            android:id="@+id/action_doctorFormFragment_to_addScheduleFragment"
            app:destination="@id/addScheduleFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            />
        <argument
            android:name="filter_doc"
            app:argType="com.example.ffccloud.FilteredDoctorInfomationModel"
            app:nullable="true"
            android:defaultValue="@null" />


    </fragment>
    <fragment
        android:id="@+id/addScheduleFragment"
        android:name="com.example.ffccloud.Target.Schedule.AddScheduleFragment"
        android:label="Add Schedule"
        tools:layout="@layout/fragment_add_schedule" >
        <argument
            android:name="doctorToSave"
            app:argType="com.example.ffccloud.SaveDoctorModel"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>
    <fragment
        android:id="@+id/showRouteFragment"
        android:name="com.example.ffccloud.TargetMenu.ShowRouteFragment"
        android:label="Routes"
        tools:layout="@layout/fragment_show_route" >
        <action
            android:id="@+id/action_showRouteFragment_to_nav_home"
            app:destination="@id/nav_home"
            app:popUpTo="@+id/nav_home"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/activityTaskFragment"
        android:name="com.example.ffccloud.Target.Task.ActivityTaskFragment"
        android:label="ActivityTaskFragment"
        tools:layout="@layout/fragment_activity_task">
        <argument
            android:name="activity_type"
            app:argType="string"
            android:defaultValue=" " />
    </fragment>
    <fragment
        android:id="@+id/meetingFragment"
        android:name="com.example.ffccloud.MeetingFragment"
        android:label="Meeting"
        tools:layout="@layout/fragment_meeting" />
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.example.ffccloud.MapsFragment"
        android:label="Tracker"
        tools:layout="@layout/fragment_maps2" />
    <fragment
        android:id="@+id/expenseFormFragment"
        android:name="com.example.ffccloud.Expense.ExpenseFormFragment"
        android:label="ExpenseFormFragment"
        tools:layout="@layout/fragment_expense_form"
        />
    <fragment
        android:id="@+id/nav_expense_list"
        android:name="com.example.ffccloud.Expense.ExpenseWalletFragment"
        android:label="Expense Wallet"
        tools:layout="@layout/fragment_expense_wallet"
        >
        <action
            android:id="@+id/action_expenseWalletFragment_to_expenseFormFragment"
            app:destination="@id/expenseFormFragment" />
    </fragment>
    <fragment
        android:id="@+id/tableLayout"
        android:name="com.example.ffccloud.TableLayout"
        android:label="Doctors Report"
        tools:layout="@layout/fragment_table_layout" />
    <fragment
        android:id="@+id/usersListFragment"
        android:name="com.example.ffccloud.Tracking.UsersListFragment"
        android:label="Users "
        tools:layout="@layout/fragment_users_list" >
        <action
            android:id="@+id/action_usersListFragment_to_trackerFragment"
            app:destination="@id/trackerFragment" />
    </fragment>
    <fragment
        android:id="@+id/locationRequestFragment"
        android:name="com.example.ffccloud.Tracking.LocationRequestFragment"
        android:label="Location Requests"
        tools:layout="@layout/fragment_location_request" />
    <fragment
        android:id="@+id/trackerFragment"
        android:name="com.example.ffccloud.Tracking.TrackerFragment"
        android:label="Tracker"
        tools:layout="@layout/fragment_tracker" >
        <argument
            android:name="recevierID"
            app:argType="string"
            app:nullable="true"
            android:defaultValue=" "/>
    </fragment>
    <fragment
        android:id="@+id/chatUserListFragment"
        android:name="com.example.ffccloud.Messages.ChatUserListFragment"
        android:label="Chats"
        tools:layout="@layout/fragment_chat_user_list" >
        <action
            android:id="@+id/action_chatUserListFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
    </fragment>
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.example.ffccloud.Messages.ChatFragment"
        android:label="Chat"
        tools:layout="@layout/fragment_chat" >
        <argument
            android:name="recevierID"
            app:argType="string"
            app:nullable="true"
            android:defaultValue=" "/>
        <argument
            android:name="recevierUserName"
            app:argType="string"
            app:nullable="true"
            android:defaultValue=" "/>
    </fragment>
    <fragment
        android:id="@+id/addCustomerFragment"
        android:name="com.example.ffccloud.Customer.AddCustomerFragment"
        android:label="fragment_add_customer"
        tools:layout="@layout/fragment_add_customer" />
</navigation>